<html>

<h1 class="jumbotron text-center">Log In</h1>
<%= form_tag sessions_path do %>

<div class="form-group">
<%= label_tag :email %>
<%= text_field_tag :email %>
</div>
  <div class="form-group">
    <%= label_tag :password %>
<%= password_field_tag :password %>
</div>

  <div class="actions">
    <%= submit_tag "Log In", class:"btn btn-primary" %>
</div>

<% end %>
</html>
